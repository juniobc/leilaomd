				<input name="Contact[verifyCode]" id="Contact_verifyCode" type="text">
				<span class="help-block error" id="Contact_verifyCode_em_" style="display: none"></span>
			</div>
			
			<div class="form-actions">
				<input class="btn btn-primary" type="submit" name="yt1" value="Enviar mensagem">            
				<button class="btn" id="yw2" type="reset" name="yt2">Limpar</button>        
